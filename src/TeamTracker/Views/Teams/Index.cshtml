@model IEnumerable<TeamTracker.Models.Team>

@{
    ViewData["Title"] = "Team Home";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Teams: </h2>
<hr />
<ul>
    @foreach (var listTeam in Model)
    {
        <li><strong>@Html.DisplayFor(modelTeam => listTeam.Name)</strong> | @Html.ActionLink("Details", "Details", new { id = listTeam.TeamId })</li>
        <br />
        
    }
</ul>